{
  "private": true,
  "name": "dipper",
  "version": "1.0.0",
  "scripts": {
    "clean": "lerna run clean",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "version": "lerna version --force-publish --conventional-commits --exact --no-changelog",
    "version:prerelease": "lerna version --force-publish --conventional-prerelease",
    "prerelease": "yarn build",
    "release": "lerna publish from-package --registry https://registry.npmjs.org/ --force-publish && lerna run sync",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "yarn clean && lerna run build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/l7-react": "^2.3.4",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "react": "^16.12.0 || ^17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/react": "^16.14.2",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.17",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lerna": "^4.0.0",
    "lerna-ci": "^0.0.8",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "yorkie": "^2.0.0"
  }
}
